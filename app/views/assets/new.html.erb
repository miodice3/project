<%= form_with model:@asset do |f|  %>
    <%= f.label :utility %>
    <%= f.text_field :utility %>
    <br><br>
    <%= f.label :utility_load_zone %>
    <%= f.text_field :zone %><br>
    <%= link_to "For MA clients looking for a hint on load zones, click here", "http://www.resonant.energy/mautilityloadzonemap" %>
    <br><br>
    <%= f.label :class_number %>
    <%= f.text_field :class_number %><br>
    Class 1 - 60kW or less<br>
    Class 2 - more than 60 kW but less than or equal to 1 MW<br>
    Class 3 - more than 1 MW but less than or equal to 2 MW<br><br>
    <%= f.label :meter_type %>
    <%= f.text_field :meter_type %><br>
     <br><br>
    <%= f.label :states_name_two_letters %>
    <%= f.text_field :state %>
    <br><br>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <br><br>
    <%= f.label :Estimate_annual_capacity_in_killowatt_hours %>
    <%= f.text_field :annual_capacity %>
    <br><br>
    <%= f.label :construction_status %>
    <%= f.text_field :status %>
    <br><br>
    
    <%= f.submit "Create New Asset" %>
<% end%>