<%= form_with model:@user do |f|  %>
    <%= f.label :name %>
    <%= f.text_field :name %> 
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.label :password_confirmation, "Confirm Password" %>
    <%= f.password_field :password_confirmation %>
    <br>
    <%= f.submit %>
<% end%>

<%= link_to "Activate by Google", '/auth/google_oath2' %>