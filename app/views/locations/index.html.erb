<% @locations.each do |l| %>
    <%= l.name %> - Owners Name: <%= l.owner.name %><br>
    <%= l.utility %>, <%= l.zone %>, meter type: <%= l.meter_type %><br>
    <%= l.state %>, <%= l.country %><br>
    <%= l.annual_capacity %> kWh/year<br>
    The Owner has marked this as: <%= l.status %><br>
    <%= link_to "Click to see details or submit contract offer", location_path(l.id) %><br>

    <br><br>
<% end %>